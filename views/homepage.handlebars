<!-- Loop over each object in the projects array -->
{{#each posts as |post| }}
<div class="row mb-4 post">
  <div class="col-md-8">
    <h2>
      <a href="/posts/{{post.id}}">{{post.title}}</a>
    </h2>
    <p>
      {{post.contents}}
    </p>
    <p>Created by {{post.user.name}} on {{post.date_created}}</p>
  </div>

</div>
{{/each}}
